<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <PreferenceCategory
        android:title="@string/pref_safetynet_recent_cat_basic">
        <Preference
            android:key="pref_request_type"
            android:selectable="false"
            android:title="@string/pref_safetynet_recent_request_type"
            tools:summary="ATTESTATION" />

        <Preference
            android:key="pref_time"
            android:selectable="false"
            android:title="@string/pref_safetynet_recent_request_time"
            tools:summary="1970/01/01 1:00 AM" />
    </PreferenceCategory>

    <PreferenceCategory
        android:title="@string/pref_safetynet_recent_cat_request">

        <Preference
            android:key="pref_nonce"
            android:selectable="false"
            android:title="@string/pref_safetynet_recent_nonce"
            tools:summary="68b329da9893e34099c7d8ad5c" />
    </PreferenceCategory>

    <PreferenceCategory
        android:title="@string/pref_safetynet_recent_cat_response">
        <Preference
            android:key="pref_status"
            android:selectable="false"
            android:title="@string/pref_safetynet_recent_response_status"
            tools:summary="CTS failed" />

        <Preference
            android:key="pref_eval_type"
            android:selectable="false"
            android:title="@string/pref_safetynet_recent_eval_type"
            tools:summary="HARDWARE_BACKED" />

        <Preference
            android:key="pref_advice"
            android:selectable="false"
            android:title="@string/pref_safetynet_recent_advice"
            tools:summary="RESTORE_TO_FACTORY_ROM" />

        <Preference
            android:key="pref_copy_result"
            android:title="@string/pref_safetynet_recent_copy_json"
            app:icon="?attr/actionModeCopyDrawable" />
    </PreferenceCategory>

</PreferenceScreen>
