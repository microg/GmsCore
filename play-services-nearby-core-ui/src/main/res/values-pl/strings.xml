    <string name="service_name_exposure">Powiadomienia o narażeniu</string>
    <string name="pref_exposure_enable_info_summary">Aby włączyć funkcję Powiadomienia o narażeniu (GAEN), otwórz dowolną aplikację ją wspierającą.</string>
    <string name="pref_exposure_error_bluetooth_off_title">Włącz Bluetooth</string>
